#!/bin/sh
#Generated by JetBrains Toolbox 1.19.7784 at 2020-12-28T09:06:31.000521

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/bartsmykla/Library/Application Support/JetBrains/Toolbox/apps/Goland/ch-0/203.5981.98/GoLand.app/Contents/MacOS/goland" $wait --args "${ideargs[@]}"